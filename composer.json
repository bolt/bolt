{
    "name" : "bolt/bolt",
    "description" : "Sophisticated, lightweight & simple CMS",
    "homepage" : "http://bolt.cm",
    "keywords" : [
        "CMS",
        "Silex",
        "PHP",
        "Symfony"
    ],
    "license" : "MIT",
    "require" : {
        "bolt/dumper" : "~2.0",
        "bolt/package-wrapper": "~1.0|~2.0",
        "bolt/pathogen" : "~0.6",
        "bolt/thumbs" : "~2.0",
        "brandonwamboldt/utilphp" : "~1.0",
        "cocur/slugify" : "~1.0",
        "composer/composer" : "^1.2",
        "doctrine/dbal" : "2.5.1",
        "erusev/parsedown-extra" : "~0.2",
        "ext-mbstring" : "*",
        "ext-pdo" : "*",
        "filp/whoops" : "~1.1",
        "hautelook/phpass" : "~0.3",
        "ircmaxell/random-lib" : "~1.1",
        "miljar/php-exif": "~0.4",
        "monolog/monolog" : "~1.12",
        "php" : ">=5.3.3",
        "rossriley/flysystem53" : "~1.1",
        "siriusphp/upload" : "~1.2",
        "silex/silex" : "1.2.*",
        "silex/web-profiler" : "1.0.4",
        "stecman/symfony-console-completion" : "~0.4",
        "swiftmailer/swiftmailer" : "~5.3",
        "symfony/browser-kit" : "<2.7",
        "symfony/config" : "<2.7",
        "symfony/console" : "<2.7",
        "symfony/css-selector" : "<2.7",
        "symfony/debug" : "<2.7",
        "symfony/dependency-injection" : "<2.7",
        "symfony/dom-crawler" : "<2.7",
        "symfony/event-dispatcher" : "<2.7",
        "symfony/filesystem" : "<2.7",
        "symfony/finder" : "<2.7",
        "symfony/form" : "<2.7",
        "symfony/http-foundation" : "<2.7",
        "symfony/intl" : "<2.7",
        "symfony/options-resolver" : "<2.7",
        "symfony/property-access" : "<2.7",
        "symfony/process" : "<2.7",
        "symfony/security-core" : "<2.7",
        "symfony/security-csrf" : "<2.7",
        "symfony/stopwatch" : "<2.7",
        "symfony/translation" : "<2.7",
        "symfony/twig-bridge" : "<2.7",
        "symfony/validator" : "<2.7",
        "symfony/var-dumper" : "<2.7",
        "symfony/web-profiler-bundle" : "<2.7",
        "symfony/yaml" : "<2.7",
        "tdammers/htmlmaid" : "~0.7",
        "twig/twig" : "~1.16",
        "ua-parser/uap-php" : "~3.4"
    },
    "require-dev" : {
        "bolt/codingstyle" : "~1.0",
        "codeception/codeception" : "<=2.1.2",
        "lstrojny/phpunit-function-mocker" : "0.3.0",
        "m6web/symfony2-coding-standard" : "dev-master@dev",
        "phpmd/phpmd" : "~2.2.0",
        "phpunit/dbunit" : "~1.3",
        "phpunit/php-code-coverage" : "~2.0",
        "phpunit/phpunit" : "^4.8",
        "sebastian/phpcpd" : "~2.0",
        "squizlabs/php_codesniffer" : "~2.0"
    },
    "config" : {
        "preferred-install" : "dist"
    },
    "autoload" : {
        "psr-4" : {
            "Bolt\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Bolt\\Tests\\" : "tests/phpunit/unit"
        }
    },
    "bin" : [
        "app/nut"
    ],
    "extra" : {
        "branch-alias" : {
            "dev-release/2.2" : "2.2.x-dev"
        }
    }
}
