PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE bolt_authtoken (id INTEGER NOT NULL, username VARCHAR(32) NOT NULL, token VARCHAR(128) NOT NULL, salt VARCHAR(128) NOT NULL, lastseen DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, ip VARCHAR(32) DEFAULT '' NOT NULL, useragent VARCHAR(128) DEFAULT '' NOT NULL, validity DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, PRIMARY KEY(id));
CREATE TABLE bolt_users (id INTEGER NOT NULL, username VARCHAR(32) NOT NULL, password VARCHAR(128) NOT NULL, email VARCHAR(128) NOT NULL, lastseen DATETIME NOT NULL, lastip VARCHAR(32) DEFAULT '' NOT NULL, displayname VARCHAR(32) NOT NULL, userlevel VARCHAR(32) NOT NULL, contenttypes VARCHAR(256) NOT NULL, stack VARCHAR(1024) DEFAULT '' NOT NULL, enabled BOOLEAN NOT NULL, shadowpassword VARCHAR(128) DEFAULT '' NOT NULL, shadowtoken VARCHAR(128) DEFAULT '' NOT NULL, shadowvalidity DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, failedlogins INTEGER DEFAULT 0 NOT NULL, throttleduntil DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, PRIMARY KEY(id));
