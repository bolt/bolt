CREATE TABLE bolt_content_changelog (id INTEGER NOT NULL, date DATETIME NOT NULL, username VARCHAR(64) DEFAULT '' NOT NULL, title VARCHAR(256) DEFAULT '' NOT NULL, contenttype VARCHAR(128) NOT NULL, contentid INTEGER NOT NULL, mutation_type VARCHAR(16) NOT NULL, diff CLOB NOT NULL, PRIMARY KEY(id));
CREATE TABLE bolt_entries (id INTEGER NOT NULL, slug VARCHAR(128) NOT NULL, datecreated DATETIME NOT NULL, datechanged DATETIME NOT NULL, datepublish DATETIME NOT NULL, datedepublish DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, username VARCHAR(32) NOT NULL, status VARCHAR(32) NOT NULL, title VARCHAR(256) DEFAULT '' NOT NULL, image VARCHAR(256) DEFAULT '' NOT NULL, teaser CLOB DEFAULT '' NOT NULL, video CLOB DEFAULT '' NOT NULL, body CLOB DEFAULT '' NOT NULL, PRIMARY KEY(id));
CREATE TABLE bolt_pages (id INTEGER NOT NULL, slug VARCHAR(128) NOT NULL, datecreated DATETIME NOT NULL, datechanged DATETIME NOT NULL, datepublish DATETIME NOT NULL, datedepublish DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, username VARCHAR(32) NOT NULL, status VARCHAR(32) NOT NULL, title VARCHAR(256) DEFAULT '' NOT NULL, image VARCHAR(256) DEFAULT '' NOT NULL, teaser CLOB DEFAULT '' NOT NULL, body CLOB DEFAULT '' NOT NULL, template VARCHAR(256) DEFAULT '' NOT NULL, PRIMARY KEY(id));
CREATE TABLE bolt_kitchensinks (id INTEGER NOT NULL, slug VARCHAR(128) NOT NULL, datecreated DATETIME NOT NULL, datechanged DATETIME NOT NULL, datepublish DATETIME NOT NULL, datedepublish DATETIME DEFAULT '1900-01-01 00:00:00' NOT NULL, username VARCHAR(32) NOT NULL, status VARCHAR(32) NOT NULL, title VARCHAR(256) DEFAULT '' NOT NULL, video CLOB DEFAULT '' NOT NULL, html CLOB DEFAULT '' NOT NULL, geolocation CLOB DEFAULT '' NOT NULL, image VARCHAR(256) DEFAULT '' NOT NULL, imagelist CLOB DEFAULT '' NOT NULL, file VARCHAR(256) DEFAULT '' NOT NULL, checkbox BOOLEAN DEFAULT '0' NOT NULL, markdown CLOB DEFAULT '' NOT NULL, textarea CLOB DEFAULT '' NOT NULL, template VARCHAR(256) DEFAULT '' NOT NULL, datetime DATETIME NOT NULL, date DATE NOT NULL, integerfield INTEGER DEFAULT 0 NOT NULL, floatfield DOUBLE PRECISION DEFAULT '0' NOT NULL, selectfield CLOB DEFAULT '' NOT NULL, multiselect CLOB DEFAULT '' NOT NULL, PRIMARY KEY(id));
