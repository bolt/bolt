    "type" : "bolt-extension",
    "keywords" : [
        "events",
        "listeners",
        "dispatchers"
    ],
    "require" : {
        "bolt/bolt" : "^3.0"
    },
    "license" : "MIT",
    "authors" : [{
            "name" : "Gawain Lynch",
